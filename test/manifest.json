{
	"manifest_version":2, 
	
	"name": "check zeroelov",
	"description":"This extension alerts the user if summoner zeroelov starts to play a game",
	"version":"1.0",
	
	"browser_action":{
		"default_icon":{
			"19":"16.png",
	     	"38":"32.png"
			},
		"default_popup": "popup.html",
		"default_title": "summoner zeroelov's league status"			   
	},
	
	"permissions": [
		"activeTab",
		"https://ajax.googleapis.com/"
	]
 
 }
	
	